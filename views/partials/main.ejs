<!-- id, title, content_type -->
<main id="main" class="flexRow vp">
    <% switch(content_type) {
        case 'form': %>
            <% const safeAreas = typeof areas !== 'undefined' ? areas : []; %>
            <%- include('components/forms', { id, title, areas: safeAreas }); %>
        <% break;
        case 'ticket_collectors': %>
            <%- include('components/ticket_collectors', { id, title }); %>
        <% break;
        case 'configurations': %>
            <%- include('components/configurations', { id, title }); %>
    <% } %>
</main>